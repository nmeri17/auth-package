{
    "name": "nmeri/auth-package",
    "description": "Suphle routes, controllers for common requirements in user account authentication e.g. registration, password recovery etc",
    "type": "library",
    "require-dev": {
        "nmeri/tilwa": "*"
    },
    "autoload": {
        "psr-4": {
            "SuphleAuth\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "SuphleAuth\\Tests\\": "tests/"
        }
    },

    "repositories": [
    	{
    		"type": "path",
	        "url": "../suphle"
	    },{
            "type": "vcs",
            "url": "https://github.com/nmeri17/QuTee"
        }
    ],
    "minimum-stability": "dev",
    "config": {
        "allow-plugins": {
            "mcaskill/composer-exclude-files": true
        }
    }
}
